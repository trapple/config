{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "$": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "spread": true
  },
  "plugins": [
    "react"
  ],
  "extends": "eslint:recommended",
  "rules": {
    "arrow-parens": [1, "always"],
    "arrow-spacing": 1,
    "camelcase": 0,
    "key-spacing": 0,
    "no-console": 0,
    "no-const-assign": 2,
    "no-class-assign": 2,
    "no-multi-spaces": 0,
    "no-new": 1,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    "no-use-before-define": 0,
    "quotes": [0, "double"],
    "react/jsx-uses-vars": 1,
    "strict": 0
  }
}
